{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 20",
    "compilerOptions": {
        "module": "Node16",
        "target": "es2022",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node"
    }
}